{
  "name": "Git Pro",
  "author": "simon hsu",
  "authorEmail": "coolchyni@gmail.com",
  "identifier": "me.dmsy.git-plugin",
  "compatibleVersion": 3,
  "bundleVersion": 2,
  "commands": [
    {
      "name": "update",
      "identifier": "update",
      "script": "update.js",
      "handlers": {
        "actions": {
          "OpenDocument": "onRun"
        }
      }
    },
    {
      "name": "unzipOnSave",
      "identifier": "unzipOnSave",
      "script": "unzipOnSave.js",
      "handlers": {
        "run": "onRun",
        "actions": {
          "DocumentSaved": "onRun"
        }
      }
    },
    {
      "name": "commit",
      "identifier": "commit",
      "shortcut": "cmd ctrl c",
      "script": "Commit.js",
      "description": "Commit your changes.",
      "icon": "icons/commit.png"
    },
    {
      "name": "push",
      "identifier": "push",
      "shortcut": "cmd ctrl p",
      "script": "Push.js",
      "description": "Push your commits to GitHub.",
      "icon": "icons/push.png"
    },
    {
      "name": "branches",
      "identifier": "branches",
      "shortcut": "cmd ctrl b",
      "script": "Branches.js",
      "description": "Manage git branches.",
      "icon": "icons/branches.png"
    },
    {
      "name": "pull",
      "identifier": "pull",
      "script": "Pull.js",
      "description": "Pull changes and branches from GitHub.",
      "icon": "icons/pull.png"
    },
    {
      "name": "init",
      "identifier": "init",
      "script": "Init.js"
    },
    {
      "name": "preferences",
      "identifier": "preferences",
      "script": "Preferences.js"
    },
    {
      "name": "terminal",
      "identifier": "terminal",
      "script": "OpenTerminal.js"
    },
    {
      "name": "remote",
      "identifier": "setremote",
      "script": "SetRemote.js"
    },
    {
      "name": "reset",
      "identifier": "reset",
      "script": "reset.js"
    },
    {
      "name": "English",
      "identifier": "english",
      "script": "enUS.js"
    },
    {
      "name": "中文",
      "identifier": "chinese",
      "script": "zhCN.js"
    }
  ],
  "menu": {
    "title": "Git pro",
    "items": [
      "commit",
      "push",
      "pull",
      "reset",
      "branches",
      {
        "title": "advance..",
        "items": [
          "init",
          "setremote",
          "terminal",
          {
            "title": "language..",
            "items": [
              "english",
              "chinese"
            ]
          }
        ]
      }
    ]
  },
  "version": "0.1.3",
  "disableCocoaScriptPreprocessor": true,
  "appcast": "https://raw.githubusercontent.com//master/.appcast.xml"
}